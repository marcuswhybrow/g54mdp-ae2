<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="net.marcuswhybrow.uni.g54mdp.ae02.addressbook"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity
            android:name="AddressBookActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ContactFormActivity"
            android:label="@string/app_contact_form_activity_name">
            <intent-filter>
                <action android:name="net.marcuswhybrow.uni.g54mdp.ae02.addressbook.CONTACT_ADD" />
                <action android:name="net.marcuswhybrow.uni.g54mdp.ae02.addressbook.CONTACT_EDIT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
</manifest> 
